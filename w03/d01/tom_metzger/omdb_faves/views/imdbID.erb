<h1>imdbID link works</h1>

<!-- this unordered list pulls values from the @result_symbols_hash and displays them -->
<ul>
  <li>Title:    <%= @result_symbols_hash[:title]    %> </li>
  <li>Year:     <%= @result_symbols_hash[:year]     %> </li>
  <li>Rated:    <%= @result_symbols_hash[:rated]    %> </li>
  <li>Released: <%= @result_symbols_hash[:released] %> </li>
  <li>Runtime:  <%= @result_symbols_hash[:runtime]  %> </li>
  <li>Genre:    <%= @result_symbols_hash[:genre]    %> </li>
  <li>Director: <%= @result_symbols_hash[:director] %> </li>
  <li>Writer:   <%= @result_symbols_hash[:writer]   %> </li>
  <li>Actors:   <%= @result_symbols_hash[:actors]   %> </li>
  <li>Plot:     <%= @result_symbols_hash[:plot]     %> </li>
</ul>

  <!-- this line pulls the image url for the movie's poster from the HTTParty request that was converted to the @result_symbols_hash -->
  <div>
    <a href="http://google.com/#q=<%= @google %>">
      <img src="<%= @result_symbols_hash[:poster] %>">
    </a>
  </div>

<form method="post" action="/add/favorite">
  <!-- <input type="hidden" name="movie_data" value="<%= @result_symbols_hash %>"> -->
  <input type="hidden" name="title" value="<%= @result_symbols_hash[:title] %>">
  <input type="hidden" name="year" value="<%= @result_symbols_hash[:year] %>">
  <input type="hidden" name="rated" value="<%= @result_symbols_hash[:rated] %>">
  <input type="hidden" name="released" value="<%= @result_symbols_hash[:released] %>">
  <input type="hidden" name="runtime" value="<%= @result_symbols_hash[:runtime] %>">
  <input type="hidden" name="genre" value="<%= @result_symbols_hash[:genre] %>">
  <input type="hidden" name="director" value="<%= @result_symbols_hash[:director] %>">
  <input type="hidden" name="writer" value="<%= @result_symbols_hash[:writer] %>">
  <input type="hidden" name="actors" value="<%= @result_symbols_hash[:actors] %>">
  <input type="hidden" name="plot" value="<%= @result_symbols_hash[:plot] %>">
  <input type="hidden" name="poster" value="<%= @result_symbols_hash[:poster] %>">
  <input type="submit" value="Add to Favorites">
</form>


