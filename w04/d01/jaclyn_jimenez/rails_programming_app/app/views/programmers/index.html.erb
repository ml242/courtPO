<h1> Programmers</h1>

<%  @programmers.each do |programmer| %>

<div class="each_programmer">
  <a href="/programmers/<%= programmer.id %>"> <img src="<%= programmer.twitter_pic %>" /> </a>
  <h2><%= programmer.name %></h2>
  <h3> Twitter: <a href="http://twitter.com/<%= programmer.twitter_username %>" > @<%= programmer.name %> </a> </h3>
  </div>
<%  end %>

