<img src="<%= @twitter_pic %>">
<p><%= @name %></p>
<a href="/programmers/<%= @id %>/edit">Edit</a>
<% unless @twitter_username.nil? %>
  <h5>Twitter:</h5>
  <a href="https://twitter.com/<%= @twitter_username %>" target="_blank">@<%= @twitter_username %></a>
<% end %>
<% unless @github_username.nil? %>
  <h5>Github:</h5>
  <a href="https://github.com/<%= @github_username %>" target="_blank">@<%= @github_username %></a>
<% end %>
