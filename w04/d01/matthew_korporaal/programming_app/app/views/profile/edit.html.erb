<h1>Edit <%= @programmer.name %><h1>

<%= form_tag("/programmers/#{@programmer.id.to_s}", method: "post") do %>
  <%= text_field_tag("prog-name", "#{@programmer.name}") %>Name<br>
  <%= text_field_tag("prog-twitter", "#{@programmer.twitter_acct}") %>Twitter Account<br>
  <%= text_field_tag("prog-github", "#{@programmer.github_acct}") %>GitHub Account<br>
  <%= text_field_tag("prog-url", "#{@programmer.avatar_url}") %>Pic URL<br>
  <%= submit_tag("Update") %>
<% end %>