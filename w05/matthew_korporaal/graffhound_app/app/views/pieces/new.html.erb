<h1>Add New Piece</h1>


<%= form_for(@piece) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.select :artist_id, options_from_collection_for_select(@artists, :id, :name) %>
<%= f.label :artist %>
<%= f.text_field :artist %>
<%= f.label :location %>
<%= f.text_field :location %>
<%= f.label :url %>
<%= f.text_field :url %><br>
<%= f.submit "Submit" %>
<% end %>