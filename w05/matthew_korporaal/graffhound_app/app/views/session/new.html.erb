<%= flash[:notice] %>

<div class="session-div">
<h1>Login</h1>
<%= form_tag('/login') do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "Login" %>
<% end %>
<p>
  Don't have an account yet?
</p>
<p>
  <%= link_to "Sign up", "/users/new" %>
</p>
</div>